import java.util.*;
class main{
public static void main(String[] args){
int a[]={-3,2,4,-1,2,3,-2};
int n=a.length;
int max=Integer.MIN_VALUE;
for(int i=0;i<n;i++){
  for(int j=1;j<n;j++){
    int cursum=0;
    for(int k=i;k<=j;k++){
      cursum+=a[k];
      }
    if(cursum>max)max=cursum;
    }
    
   }System.out.print(max);
  }
 }
